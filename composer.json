{
    "name": "bachphuc/php-laravel-helpers",
    "description": "php helper for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "bachphuc",
            "email": "trieuquangphuc123@gmail.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "bachphuc\\PhpLaravelHelpers\\": "src/"
        },
        "files": [
            "src/helpers.php",
            "src/str_helpers.php",
            "src/model_helpers.php",
            "src/datetime_helpers.php",
            "src/route_helpers.php",
            "src/image_helpers.php",
            "src/auth_helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "aliases": {
                "LaravelHelpers": "bachphuc\\PhpLaravelHelpers"
            }
        }
    },
    "minimum-stability": "stable"
}
